{
  "compilerOptions": {
    "target": "ES6",
    "allowSyntheticDefaultImports": true,
    "module": "commonJS",
    "baseUrl": ".",
    "paths": {
      "*": ["*"],
      "@bigbinary/neetoui/*": ["*"],
      "src/*": ["src/*"],
      "src": ["src"],
      "atoms/*": ["src/atoms/*"],
      "atoms": ["src/atoms"],
      "components/*": ["src/components/*"],
      "components": ["src/components"],
      "constants/*": ["src/constants/*"],
      "constants": ["src/constants"],
      "hooks/*": ["src/hooks/*"],
      "hooks": ["src/hooks"],
      "managers/*": ["src/managers/*"],
      "managers": ["src/managers"],
      "utils/*": ["src/utils/*"],
      "utils": ["src/utils"],
    },
    "esModuleInterop": true,
    "jsx": "react"
  },
  "exclude": ["build", "node_modules", "storybook-static"],
  "include": ["./**/*.js", "./**/*.jsx"]
}
