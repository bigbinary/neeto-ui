{
  "compilerOptions": {
    "target": "ES6",
    "allowSyntheticDefaultImports": true,
    "module": "commonJS",
    "baseUrl": ".",
    "paths": {
      "*": ["*"],
      "@bigbinary/neetoui/*": ["*"],
      "lib/*": ["lib/*"],
      "lib": ["lib"],
      "atoms/*": ["lib/atoms/*"],
      "atoms": ["lib/atoms"],
      "components/*": ["lib/components/*"],
      "components": ["lib/components"],
      "constants/*": ["lib/constants/*"],
      "constants": ["lib/constants"],
      "hooks/*": ["lib/hooks/*"],
      "hooks": ["lib/hooks"],
      "managers/*": ["lib/managers/*"],
      "managers": ["lib/managers"],
      "utils/*": ["lib/utils/*"],
      "utils": ["lib/utils"],
    },
    "esModuleInterop": true,
    "jsx": "react"
  },
  "exclude": ["build", "node_modules", "storybook-static"],
  "include": ["./**/*.js", "./**/*.jsx"]
}
